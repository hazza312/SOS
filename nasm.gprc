configuration project NASM is
   package Compiler is
      for Driver ("Asm") use "nasm";
      for Leading_Required_Switches ("Asm") use ("-c");
      for Object_File_Suffix ("Asm") use ".o";
      for Dependency_Switches ("Asm") use ("-MMD","-MF","");
      for Include_Switches ("Asm") use ("-I");
   end Compiler;
   package Naming is
      for Body_Suffix ("Asm") use ".asm";
   end Naming;
end NASM;