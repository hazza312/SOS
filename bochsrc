cpu: model=corei7_sandy_bridge_2600k
debug_symbols: file=dist/kernel.sym
display_library: x, options="gui_debug"
# We give our system 32 Mb of RAM.
megs: 512

# This is the additional package we installed. We add it into our configuration.
display_library: sdl


ata0-slave:  type=cdrom, path=dist/os.iso, status=inserted  

boot: cdrom

# Logfile
log: bochsout.txt

# We don't need mouse emulation
mouse: enabled=0